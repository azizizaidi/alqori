{
    "name": "apility/laravel-ngrok",
    "description": "Share Laravel application with ngrok.",
    "keywords": [
        "apility",
        "laravel-ngrok"
    ],
    "homepage": "https://github.com/apility/laravel-ngrok",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        },
        {
            "name": "Jairo Correa",
            "email": "jn.j41r0@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/console": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/pagination": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Apility\\Laravel\\Ngrok\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apility\\Laravel\\Ngrok\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apility\\Laravel\\Ngrok\\NgrokServiceProvider"
            ]
        }
    }
}